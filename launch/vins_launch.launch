<launch>
      <!-- use sim time-->
      <param name="use_sim_time" value="true" />

      <arg name="node_start_delay" default="1.0" />
      <node pkg="rosbag" type="play" name="player"
            args="--clock --hz=1000  $(find cerberus2)/bags/legged_robots/20230215_lab_short.bag" />

      <!-- VINS config -->
      <param name="vins_config_file" type="string"
            value="$(find cerberus2)/config/go1_config/hardware_go1_vilo_config.yaml" />

      <node name="vinsfusion" pkg="vins" type="vins_node"
            args="$(find cerberus2)/config/go1_config/hardware_go1_vilo_config.yaml" output="screen" />
</launch>